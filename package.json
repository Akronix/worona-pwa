    "react-dom": "^15.5.4",
    "react-google-tag-manager": "^2.1.0",
    "react-redux": "^5.0.5",
    "recompose": "^0.23.5",
    "redux": "^3.7.1",
    "redux-saga": "^0.15.4",
    "replace-in-file": "^2.5.3",
    "reselect": "^3.0.1",
