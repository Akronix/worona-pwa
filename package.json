  "main": "index.js",
  "scripts": {
    "rec-install": "npm-recursive-install --skip-root",
    "dev": "env-cmd ./env.config.js node server/index.js",
    "dev:wp": "env-cmd ./env.config.js node server/index.js --wp",
    "dev:debug": "env-cmd ./env.config.js node --inspect server/index.js",
    "dev:wp:debug": "env-cmd ./env.config.js node --inspect server/index.js --wp",
    "build": "env-cmd ./env.config.js next build --prod",
    "build:wp": "env-cmd ./env.config.js next build --prod --wp",
    "start": "env-cmd ./env.config.js node server/index.js --prod",
    "start:wp": "env-cmd ./env.config.js node server/index.js --prod --wp",
    "now-build": "npm run rec-install && env-cmd ./env.config.js next build --prod",
    "now-start": "env-cmd ./env.config.js node server/index.js --prod",
    "fix-permissions": "chmod -R u+x node_modules/@worona/next/dist/bin"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
